<metal:page define-macro="master">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      lang="en-US"
      metal:use-macro="here/main_template/macros/master"> 

<metal:block metal:fill-slot="top_slot" define-slot="top_slot"
             tal:define="dummy python:request.set('disable_border',1)" />
						 
<metal:fillbase fill-slot="base">
  <metal:baseslot define-slot="base">
    <base href="" tal:attributes="href template/absolute_url" />
  </metal:baseslot>
</metal:fillbase>


    <metal:javascriptslot fill-slot="javascript_head_slot">
     <!--  <script type="text/javascript" src="ajax.js"> </script> -->
	  <script type="text/javascript" >
	       <!-- CUSTOM JAVASCRIPT -->
			function createFromMultiSelection(src, dest)
			{	
				themes=document.getElementById(src).value;
				if(document.getElementById(dest).value=="")
				{
					document.getElementById(dest).value= themes ;
				}
				else
				{
					document.getElementById(dest).value= document.getElementById(dest).value +"|"+themes;
				}	
			}
			function popitup() 
			{
				ELDIS_GUID= document.getElementById("ELDIS_GUID").value; 
				ELDIS_COUNTRY= document.getElementById("ELDIS_COUNTRY").value;
				ELDIS_THEME= document.getElementById("ELDIS_THEME").value;
            ELDIS_PUBLISHED_AFTER= document.getElementById("ELDIS_PUBLISHED_AFTER").value;
            ELDIS_PUBLISHED_BEFORE= document.getElementById("ELDIS_PUBLISHED_BEFORE").value;
            ELDIS_PUBLISHED_YEAR= document.getElementById("ELDIS_PUBLISHED_YEAR").value;				
				newwindow=window.open('preview.html?extra=guid:'+ELDIS_GUID+'!country:'+ELDIS_COUNTRY+'!theme:'+ELDIS_THEME+'!ELDIS_PUBLISHED_AFTER:'+ELDIS_PUBLISHED_AFTER +'!ELDIS_PUBLISHED_BEFORE:'+ELDIS_PUBLISHED_BEFORE+ '!ELDIS_PUBLISHED_YEAR:'+ELDIS_PUBLISHED_YEAR,'name','height=600px,width=600px');
				if (window.focus) {newwindow.focus();}
				return false;
			}
			<!--END CUSTOM JAVASCRIPT -->
	  </script>
    </metal:javascriptslot>

    <metal:override fill-slot="portlets_one_slot">
      <metal:prefs use-macro="here/portlet_prefs/macros/portlet" />
    </metal:override>

