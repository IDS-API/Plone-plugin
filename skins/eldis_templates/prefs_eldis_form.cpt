						<!-- simulating views -->
						<ul class="contentViews"
							 tal:define="tabs python:('Config','prefs_eldis_form'),('Management','prefs_eldis_manage_form'),('Document','prefs_eldis_document'),;
													 tabs python:[ {'label':label, 'name':name} for (label,name) in tabs ];">
								<li tal:repeat="tab tabs"
												class="selected" tal:attributes="class python:template.pt_source_file()==tab['name'] and 'selected' or ''">
									<a href="" tal:attributes="href string:${here/absolute_url}/${tab/name}" i18n:translate="" tal:content="tab/label">Config</a>
